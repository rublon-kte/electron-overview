{
  "name": "electron-quick-start",
  "version": "1.0.2",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:app": "electron-packager . electron-quick-start --platform win32 --arch x64 --out dist/ --overwrite",
    "build:setup": "electron-installer-windows --src dist/electron-quick-start-win32-x64/ --dest dist/installers/",
    "build:app-setup": "npm run build:app && npm run build:setup"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-installer-windows": "^3.0.0"
  },
  "dependencies": {
    "electron-packager": "^15.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "jshint": "^2.13.0"
  }
}
